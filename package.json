{
  "name": "whoisinjail-backend",
  "version": "1.0.0",
  "description": "This is a public directory for Nigerian Prisons",
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "test": "cross-env NODE_ENV=test nyc --reporter=text --reporter=html mocha src/test/*.js --timeout 10000 -c --require @babel/register --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "start": "node dist/index.js",
    "build": "babel src -d dist",
    "migrate": "sequelize db:migrate ",
    "migrate:reset": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "migrate:seed": "npm run migrate && npm run seed",
    "start:dev": "npm run migrate && nodemon --exec babel-node ./src/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pseudogarden/whoisinjail-backend.git"
  },
  "keywords": [
    "whoisinjail",
    "Backend",
    "Nigerian",
    "Prison"
  ],
  "author": "whoisinjail team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pseudogarden/whoisinjail-backend/issues"
  },
  "homepage": "https://github.com/pseudogarden/whoisinjail-backend#readme",
  "dependencies": {
    "@babel/polyfill": "^7.4.4",
    "@hapi/joi": "^15.1.1",
    "@sendgrid/mail": "^6.4.0",
    "@types/hapi__joi": "^15.0.3",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "cloudinary": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "dotenv": "^8.0.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "exceljs": "^4.1.1",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "joi-password-complexity": "^3.1.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "moment": "^2.27.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "paystack": "^2.0.1",
    "paystack-node": "^0.2.4",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "request": "^2.87.0",
    "sequelize": "^5.15.0",
    "sequelize-cli": "^5.5.0",
    "underscore": "^1.9.1",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.6.2",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.6",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "faker": "^4.1.0",
    "mocha": "^7.1.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^2.0.2",
    "nyc": "^14.1.1"
  }
}
